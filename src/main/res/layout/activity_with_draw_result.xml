<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/little_gray"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include layout="@layout/include_head_view" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:layout_width="@dimen/magin_80"
            android:layout_height="@dimen/magin_80"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/magin_50"
            android:src="@mipmap/tixian_tixianchenggong" />

        <TextView
            style="@style/style_text_style_pingfang_changgui"
            android:id="@+id/canWithDrawMoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/magin_20"
            android:text="提现成功"
            android:textColor="@color/text_black"
            android:textSize="@dimen/largeTextSize" />

        <TextView
            style="@style/style_text_style_pingfang_changgui"
            android:id="@+id/mTimeTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/magin_5"
            android:layout_marginBottom="@dimen/magin_50"
            android:text="预计1-3个工作日到账"
            android:textColor="@color/text_gray"
            android:textSize="@dimen/smallTextSize" />
    </LinearLayout>

    <TextView
        style="@style/style_text_style_pingfang_changgui"
        android:id="@+id/withDraw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_marginRight="@dimen/margin_10"
        android:background="@drawable/shape_solid_app_corlor_"
        android:gravity="center"
        android:padding="@dimen/magin_8"
        android:text="完成"
        android:textColor="@color/white"
        android:textSize="@dimen/largeTextSize" />
</LinearLayout>